 433:tmk_core/protocol/chibios/usb_main.c ****     while (usb_event_queue_dequeue(&event)) {
 434:tmk_core/protocol/chibios/usb_main.c ****         switch (event) {
 435:tmk_core/protocol/chibios/usb_main.c ****             case USB_EVENT_SUSPEND:
