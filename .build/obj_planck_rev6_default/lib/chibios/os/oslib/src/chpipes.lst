 174:lib/chibios/os/oslib/src/chpipes.c ****     pp->rdptr += n;
 175:lib/chibios/os/oslib/src/chpipes.c ****   }
 176:lib/chibios/os/oslib/src/chpipes.c ****   else if (n > s1) {
