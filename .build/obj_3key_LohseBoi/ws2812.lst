  51:platforms/avr/drivers/ws2812.c **** /*
  52:platforms/avr/drivers/ws2812.c ****   This routine writes an array of bytes with RGB values to the Dataout pin
  53:platforms/avr/drivers/ws2812.c ****   using the fast 800kHz clockless WS2811/2812 protocol.
  54:platforms/avr/drivers/ws2812.c **** */
  55:platforms/avr/drivers/ws2812.c **** 
  56:platforms/avr/drivers/ws2812.c **** // Timing in ns
  57:platforms/avr/drivers/ws2812.c **** #define w_zeropulse 350
  58:platforms/avr/drivers/ws2812.c **** #define w_onepulse 900
  59:platforms/avr/drivers/ws2812.c **** #define w_totalperiod 1250
  60:platforms/avr/drivers/ws2812.c **** 
  61:platforms/avr/drivers/ws2812.c **** // Fixed cycles used by the inner loop
  62:platforms/avr/drivers/ws2812.c **** #define w_fixedlow 2
  63:platforms/avr/drivers/ws2812.c **** #define w_fixedhigh 4
  64:platforms/avr/drivers/ws2812.c **** #define w_fixedtotal 8
  65:platforms/avr/drivers/ws2812.c **** 
  66:platforms/avr/drivers/ws2812.c **** // Insert NOPs to match the timing, if possible
  67:platforms/avr/drivers/ws2812.c **** #define w_zerocycles (((F_CPU / 1000) * w_zeropulse) / 1000000)
  68:platforms/avr/drivers/ws2812.c **** #define w_onecycles (((F_CPU / 1000) * w_onepulse + 500000) / 1000000)
  69:platforms/avr/drivers/ws2812.c **** #define w_totalcycles (((F_CPU / 1000) * w_totalperiod + 500000) / 1000000)
  70:platforms/avr/drivers/ws2812.c **** 
  71:platforms/avr/drivers/ws2812.c **** // w1_nops - nops between rising edge and falling edge - low
  72:platforms/avr/drivers/ws2812.c **** #if w_zerocycles >= w_fixedlow
  73:platforms/avr/drivers/ws2812.c **** #    define w1_nops (w_zerocycles - w_fixedlow)
  74:platforms/avr/drivers/ws2812.c **** #else
  75:platforms/avr/drivers/ws2812.c **** #    define w1_nops 0
  76:platforms/avr/drivers/ws2812.c **** #endif
  77:platforms/avr/drivers/ws2812.c **** 
  78:platforms/avr/drivers/ws2812.c **** // w2_nops - nops between fe low and fe high
  79:platforms/avr/drivers/ws2812.c **** #if w_onecycles >= (w_fixedhigh + w1_nops)
  80:platforms/avr/drivers/ws2812.c **** #    define w2_nops (w_onecycles - w_fixedhigh - w1_nops)
  81:platforms/avr/drivers/ws2812.c **** #else
  82:platforms/avr/drivers/ws2812.c **** #    define w2_nops 0
  83:platforms/avr/drivers/ws2812.c **** #endif
  84:platforms/avr/drivers/ws2812.c **** 
  85:platforms/avr/drivers/ws2812.c **** // w3_nops - nops to complete loop
  86:platforms/avr/drivers/ws2812.c **** #if w_totalcycles >= (w_fixedtotal + w1_nops + w2_nops)
  87:platforms/avr/drivers/ws2812.c **** #    define w3_nops (w_totalcycles - w_fixedtotal - w1_nops - w2_nops)
  88:platforms/avr/drivers/ws2812.c **** #else
  89:platforms/avr/drivers/ws2812.c **** #    define w3_nops 0
  90:platforms/avr/drivers/ws2812.c **** #endif
  91:platforms/avr/drivers/ws2812.c **** 
  92:platforms/avr/drivers/ws2812.c **** // The only critical timing parameter is the minimum pulse length of the "0"
  93:platforms/avr/drivers/ws2812.c **** // Warn or throw error if this timing can not be met with current F_CPU settings.
  94:platforms/avr/drivers/ws2812.c **** #define w_lowtime ((w1_nops + w_fixedlow) * 1000000) / (F_CPU / 1000)
  95:platforms/avr/drivers/ws2812.c **** #if w_lowtime > 550
  96:platforms/avr/drivers/ws2812.c **** #    error "Light_ws2812: Sorry, the clock speed is too low. Did you set F_CPU correctly?"
  97:platforms/avr/drivers/ws2812.c **** #elif w_lowtime > 450
  98:platforms/avr/drivers/ws2812.c **** #    warning "Light_ws2812: The timing is critical and may only work on WS2812B, not on WS2812(S)."
  99:platforms/avr/drivers/ws2812.c **** #    warning "Please consider a higher clockspeed, if possible"
 100:platforms/avr/drivers/ws2812.c **** #endif
