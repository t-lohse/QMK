 116:lib/chibios/os/hal/ports/STM32/LLD/USBv1/hal_usb_lld.c ****   usbp->pmnext += (size + 1) & ~1;
 117:lib/chibios/os/hal/ports/STM32/LLD/USBv1/hal_usb_lld.c ****   osalDbgAssert(usbp->pmnext <= STM32_USB_PMA_SIZE, "PMA overflow");
