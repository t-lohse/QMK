 182:lib/chibios/os/oslib/src/chpipes.c ****   }
 183:lib/chibios/os/oslib/src/chpipes.c ****   else {
 184:lib/chibios/os/oslib/src/chpipes.c ****     memcpy((void *)bp, (void *)pp->rdptr, n);
